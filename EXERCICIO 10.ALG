algoritmo "Exercicio 10"
var
  A, L, Area, T: Real


inicio
  Escreva ("Altura da parede: ")
  Leia(A)
  Escreva ("Largura da parede: ")
  Leia(L)
  Area <- A * L
  T <- Area / 2
  Escreval("Area a ser pintada: ",Area," M²")
  Escreva("Quantidade de tinta necessária: ",T, " litros")

fimalgoritmo