algoritmo "EXERCICIO 10"
var
A, B, C: Real
Delta: Real


inicio
  Escreva("Escreva o valor de A: ")
  Leia (A)
  Escreva("Escreva o valor de B: ")
  Leia (B)
  Escreva("Escreva o valor de C: ")
  Leia (C)
  Delta <- B^2 - (4 * A * C)
  Escreva ("O valor de Delta é: ", Delta)

  

fimalgoritmo